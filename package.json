{
  "name": "gulp-config",
  "version": "0.0.1",
  "description": "gulp config tools",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "browserify src/index.js -t [babelify] --bare > dist/index.js",
    "test": "mocha -R spec -r should --compilers js:babel/register --check-leaks test/**/*.spec.js",
    "t": "gulp gulp-mocha"
  },
  "author": "LeiZeng",
  "license": "ISC",
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-runtime": "^5.5.8",
    "babelify": "^6.1.2",
    "browserify": "^10.2.4",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-mocha": "^2.1.2",
    "gulp-sass": "^2.0.1",
    "run-sequence": "^1.1.0",
    "should": "^6.0.3"
  },
  "dependencies": {
    "del": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-util": "^3.0.5",
    "lodash": "^3.9.3",
    "through2": "^2.0.0",
    "vinyl-fs": "^1.0.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      {
        "optional": [
          "runtime"
        ]
      }
    ]
  }
}
