{
  "name": "gulp-config2",
  "version": "0.1.8",
  "description": "gulp config tools",
  "main": "dist/conf/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "babel src -e 0 --out-dir dist/conf && babel tasks -e 0 --out-dir dist/tasks",
    "test": "mocha -R spec -r should --compilers js:babel/register --check-leaks test/**/*.spec.js"
  },
  "author": "LeiZeng",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.5.8",
    "gulp-eslint": "^0.14.0",
    "gulp-mocha": "^2.1.2",
    "mocha": "^2.2.5",
    "should": "^6.0.3"
  },
  "dependencies": {
    "del": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-util": "^3.0.5",
    "lodash": "^3.9.3",
    "merge-stream": "^0.1.7",
    "run-sequence": "^1.1.0",
    "through2": "^2.0.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      {
        "stage": 0,
        "optional": [
          "runtime"
        ]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LeiZeng/gulp-config2.git"
  },
  "keywords": [
    "gulp",
    "config"
  ],
  "bugs": {
    "url": "https://github.com/LeiZeng/gulp-config2/issues"
  },
  "homepage": "https://github.com/LeiZeng/gulp-config2"
}
